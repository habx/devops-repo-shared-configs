{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "github>habx/devops-repo-shared-configs//renovate/global"
    ],
    "hostRules": [
        {
            "platform": "pypi",
            "endpoint": "https://pypi.fury.io/habxtech/",
            "password": "",
            "encrypted": {
                "username": ""
            }
        }
    ],
    "python": {
        "registryUrls": [
            "https://pypi.fury.io/habxtech/",
            "https://pypi.org/simple/"
        ]
    },
    "enabledManagers": [
        "circleci",
        "dockerfile",
        "pipenv",
        "pip_setup"
    ],
    "packageRules": [
        {
            "updateTypes": [
                "minor",
                "patch"
            ],
            "depTypeList": [
                "dependencies",
                "devDependencies"
            ],
            "automerge": true,
            "labels": [
                "automerge-noupdate"
            ]
        }
    ]
}