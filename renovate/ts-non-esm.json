{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "packageRules": [
        {
            "description": "https://github.com/sindresorhus/p-limit",
            "matchPackageNames": [
                "p-limit"
            ],
            "allowedVersions": "<4"
        },
        {
            "description": "https://github.com/sindresorhus/p-map",
            "matchPackageNames": [
                "p-map"
            ],
            "allowedVersions": "<5"
        },
        {
            "description": "https://github.com/sindresorhus/p-reduce",
            "matchPackageNames": [
                "p-reduce"
            ],
            "allowedVersions": "<3"
        },
        {
            "description": "https://github.com/sindresorhus/p-queue",
            "matchPackageNames": [
                "p-queue"
            ],
            "allowedVersions": "<7"
        },
        {
            "description": "https://github.com/sindresorhus/p-filter",
            "matchPackageNames": [
                "p-filter"
            ],
            "allowedVersions": "<3"
        },
        {
            "description": "https://github.com/sindresorhus/p-whilst",
            "matchPackageNames": [
                "p-whilst"
            ],
            "allowedVersions": "<3"
        },
        {
            "description": "https://github.com/sindresorhus/got",
            "matchPackageNames": [
                "got"
            ],
            "allowedVersions": "<12"
        },
        {
            "description": "https://github.com/sindresorhus/string-hash",
            "matchPackageNames": [
                "@sindresorhus/string-hash"
            ],
            "allowedVersions": "<2"
        },
        {
            "description": "https://github.com/node-fetch/node-fetch",
            "matchPackageNames": [ "node-fetch" ],
            "allowedVersions": "<3"
        },
        {
            "description": "https://github.com/octet-stream/form-data-encoder",
            "matchPackageNames": [ "form-data-encoder" ],
            "allowedVersions": "<2"
        },
        {
            "description": "https://www.npmjs.com/package/formdata-node",
            "matchedPackageNames": [ "formdata-node" ],
            "allowedVersions": "<5"
        }
    ]
}
