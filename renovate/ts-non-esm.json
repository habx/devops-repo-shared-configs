{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "packageRules": [
        {
            "description": "https://github.com/sindresorhus/p-limit",
            "matchPackageNames": [
                "p-limit"
            ],
            "allowedVersions": "<4"
        },
        {
            "description": "https://github.com/sindresorhus/p-map",
            "matchPackageNames": [
                "p-map"
            ],
            "allowedVersions": "<5"
        },
        {
            "matchPackageNames": [
                "p-reduce"
            ],
            "allowedVersions": "<3"
        },
        {
            "description": "ttps://github.com/sindresorhus/got",
            "matchPackageNames": [
                "got"
            ],
            "allowedVersions": "<12"
        },
        {
            "matchPackageNames": [
                "@sindresorhus/string-hash"
            ],
            "allowedVersions": "<2"
        },
        {
            "matchPackageNames": [ "node-fetch" ],
            "allowedVersions": "<3"
        }
    ]
}
